<% if (fieldRaw === 'notes') { %>
  <textarea
  required
  class="form-control my-2"
  placeholder="<%= fieldHuman %>"
  name="<%= fieldRaw %>"
  id="<%= fieldRaw %>-input"
  ></textarea>
<% } else { %>
  <input 
  required
  class="form-control my-2"
  list="<%= fieldRaw %>-list"
  placeholder="<%= fieldHuman %>"
  name="<%= fieldRaw %>"
  id="<%= fieldRaw %>-input"
  <%# Change input type by field %>
  <% if (fieldHuman === 'Timestamp') { %>
    type="datetime-local" 
  <% } else if (['Latitude', 'Longitude', 'Fatailities'].includes(fieldHuman)) { %>
    type="number" 
  <% } else { %> 
    type="text"
  <% } %>
  />
  <%# If it has unique vals, add them %>
  <% if (unique) { %>
    <datalist id="<%= fieldRaw %>-list">
      <% unique.forEach(val => { %>
        <option value="<%= val %>">
      <% }) %>
    </datalist>
  <% } %>
<% } %>

